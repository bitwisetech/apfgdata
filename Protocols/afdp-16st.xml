<?xml version="1.0"?>

<!-- Fgfs generic IO protocol file sends a set of standard names to python-tcl  viewer -->
<!--    use e.g. Aircraft/Instruments/afdi-12ch.xml to map props to these names --> 

<PropertyList>
 <generic>
  <output>
   <line_separator>newline</line_separator>
   <var_separator>|</var_separator>

   <chunk>
    <name>Ch0</name>
